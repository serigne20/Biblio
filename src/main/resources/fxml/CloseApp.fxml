<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="189.0" prefWidth="413.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ViewControllers.FileMenuController">
   <children>
      <AnchorPane prefHeight="192.0" prefWidth="414.0">
         <children>
            <Button layoutX="60.0" layoutY="125.0" mnemonicParsing="false" onAction="#Quit" prefHeight="30.0" prefWidth="82.0" text="Oui">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Label layoutX="-1.0" layoutY="37.0" prefHeight="42.0" prefWidth="413.0" text="                   Voulez vous vraiment quitter l'application ?" textAlignment="CENTER">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Button fx:id="closeButton" layoutX="260.0" layoutY="125.0" mnemonicParsing="false" onAction="#NoQuit" prefHeight="30.0" prefWidth="82.0" text="Non">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</VBox>
